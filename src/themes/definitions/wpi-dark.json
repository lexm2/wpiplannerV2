{
  "name": "WPI Dark",
  "id": "wpi-dark",
  "description": "Dark mode theme with WPI accent colors",
  "colors": {
    "primary": "#d4424f",
    "primaryHover": "#ac2b37",
    "primaryLight": "#e85a66",
    "secondary": "#2c2c2e",
    "secondaryHover": "#3a3a3c",
    "background": "#1c1c1e",
    "backgroundAlt": "#2c2c2e",
    "surface": "#2c2c2e",
    "surfaceHover": "#3a3a3c",
    "text": "#ffffff",
    "textSecondary": "#98989d",
    "textInverse": "#1d1d1f",
    "border": "#3a3a3c",
    "borderHover": "#48484a",
    "success": "#30d158",
    "warning": "#ff9f0a",
    "error": "#ff453a",
    "info": "#64d2ff"
  },
  "typography": {
    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
    "fontFamilyMono": "'SF Mono', Monaco, 'Cascadia Code', monospace"
  },
  "spacing": {
    "baseUnit": "1rem",
    "headerHeight": "64px",
    "sidebarWidth": "280px",
    "rightPanelWidth": "320px"
  },
  "effects": {
    "borderRadius": "6px",
    "borderRadiusLarge": "8px",
    "shadow": "0 2px 8px rgba(0,0,0,0.3)",
    "shadowHover": "0 4px 16px rgba(212, 66, 79, 0.2)",
    "transition": "all 0.2s ease"
  }
}